<article>
    <form action="/raffler" method="post" id="raffle-input-form">
        <label for="emails">
            Emails Here (one per line):
        </label>
        <textarea id="emails" name="emails" rows="10" placeholder="emails for raffle">{{emails}}</textarea>
        <button type="submit" class="submit-button">Run Raffle</button>
    </form>
    <div>
        <h3>Raffle Winner:</h3>
        <p id="raffle-winner-text">{{raffleWinner}}</p>
    </div>
</article>
